{
  "name": "divyansh-international",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "dotenv -e .env.local -- prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "update:all": "pnpm update --latest --recursive",
    "type-check": "tsc --noEmit",
    "seed": "tsx scripts/seed-sanity.ts",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:pull": "dotenv -e .env.local -- prisma db pull",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:migrate:reset": "dotenv -e .env.local -- prisma migrate reset",
    "db:reset": "dotenv -e .env.local -- prisma db push --force-reset",
    "db:seed": "dotenv -e .env.local -- prisma db seed"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@react-pdf/renderer": "^4.3.1",
    "@sanity/client": "^7.13.1",
    "@sanity/image-url": "^2.0.1",
    "@sanity/vision": "^4.19.0",
    "@supabase/supabase-js": "^2.86.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.25",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lenis": "^1.3.15",
    "next": "16.0.6",
    "next-sanity": "^11.6.10",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.67.0",
    "react-pdf": "^10.2.0",
    "resend": "^6.5.2",
    "sanity": "^4.19.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@prisma/config": "^7.0.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.6",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.3",
    "prisma": "^7.0.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
